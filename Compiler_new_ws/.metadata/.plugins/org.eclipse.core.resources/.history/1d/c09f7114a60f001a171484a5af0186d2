
    int arr[5000005];
    int N, Q;
    int queries[5000005];
    int p, P;
    int MOD;
    int answers[5000005];
    int q, i, j, k, l, r, m;

    MOD = 10;
    Q = 10;
    N = 10;
    P = 37;
	


    p = 1;
    loop i = 0, N {
        p = p * P % MOD;
        if i != 0 {
            arr[i] = (arr[i - 1] + p % 100) % MOD;
        }
        else{
            arr[i] = p % 10;
        }
    }
    p = 1;
    loop i = 0, Q {
        p = p * P % MOD;
        queries[i] = p;
    }

	

    loop i = 0, Q {

        q = queries[i];
        l = 0;
        r = N - 1;
        answers[i] = -1;

        loop l < r {
            m = (l + r) / 2;
            if arr[m] == q {
                answers[i] = m;
                l = 1;
                r = 0;
            }
            else {
                if arr[m] > q {
                    r = m - 1;
                }
                else {
                    l = m + 1;
                }
            }
        }
        print "Value of r :: ", r;
    }
