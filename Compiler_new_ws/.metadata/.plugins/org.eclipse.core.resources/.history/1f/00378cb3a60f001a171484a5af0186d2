    int N, arr[10];
    int i, j, temp;
    int p, P;



    P = 31;
    p = 1;

    N = 20;
    loop i = 0, N {
        arr[i] = p;
        p = (p * P) % 12;
    }

    loop i = 0, i < N {
        loop j = 0, N - i - 1 {
            if arr[j] > arr[j + 1] {
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
    
    loop i=0, N, i + 1{

     print "Value :: ", arr[i];
    
    }


