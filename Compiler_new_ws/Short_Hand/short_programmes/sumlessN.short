int sum(int limit){
int number = 2;
int count = 0;
long sum = 0;
loop(;count < limit;){
if(isPrimeNumber(number)){
sum += number;
count++;
}
number++;
}
return sum;
}
private boolean isPrimeNumber(int number){
loop(int i=2; i<=number/2; i++){
if(number % i == 0){
return false;
}
}
return true;
}
